import javafx.scene.image.Image;
import javafx.scene.image.ImageView;

public class Basket {
	private int mySpeed;
	private int myImage;
	private ImageView myBasket;
	
	public Basket(int speed) {
		mySpeed = speed;
		Image myImage = new Image(getClass().getClassLoader().getResourceAsStream("basket.png"));
		myBasket = new ImageView(myImage);
	}	
	public int getMySpeed(){
		return mySpeed;
	}
}
